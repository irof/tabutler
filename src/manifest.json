{
  "manifest_version": 2,
  "name": "Tabutler",
  "version": "1.3.0",
  "description": "sweep / order tabs",
  "icons": {
    "128": "icons/tabutler128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage"]
}
